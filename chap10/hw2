#include <stdio.h>
#include <stdlib.h>

struct node {
    int data;
    struct node *next;
};

void push(struct node **top, int data) {
    struct node *newNode = (struct node*)malloc(sizeof(struct node));
    newNode->data = data;
    newNode->next = *top;   
    *top = newNode;         
}

int pop(struct node **top) {
    if (*top == NULL) {
        printf("Stack is empty!\n");
        return -1;
    }

    struct node *temp = *top;
    int value = temp->data;

    *top = temp->next;   
    free(temp);          

    return value;
}
